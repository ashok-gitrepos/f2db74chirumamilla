extends layout

block content
  h1= title
  p Welcome to #{title}
  p This is Ashok Chirumamilla working on Assignment10.
  button(type="button", class="btn btn-info" onClick="doBump()") Press Me

  div#bump Bump the count

  script.
    var count=0
    var bump=1
    function doBump(){
      count=count+bump
      bump=bump+1
      document.getElementById("bump").innerHTML= "Count "+count+" Bump "+bump
    }